<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.dao.mimaDao">
   <select id="mimalist" resultType="com.qf.pojo.mima">
	   select * from mima
   </select>
    <select id="getmima" resultType="com.qf.pojo.mima" parameterType="java.lang.String">
        select * from mima where uname=#{name}
    </select>



</mapper>